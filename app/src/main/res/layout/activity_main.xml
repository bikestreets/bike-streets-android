<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.mapbox.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="39.7326381"
        mapbox:mapbox_cameraTargetLng="-104.9687837"
        mapbox:mapbox_cameraZoom="12.0" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/bottom_sheet_fragment"
        android:name="com.application.bikestreets.bottomsheet.BottomSheetFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/settings_fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="12dp"
        tools:visibility="gone" />


</androidx.constraintlayout.widget.ConstraintLayout>

